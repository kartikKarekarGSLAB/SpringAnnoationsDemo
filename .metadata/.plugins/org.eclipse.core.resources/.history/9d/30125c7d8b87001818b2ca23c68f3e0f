package com.gslab.beansdemo.model;

public class Employee {
	private int id;
	private String name;
	private String designation;
	private float salary;
	public Employee() {
	}
	public Employee(int id, String name, String designation, float salary) {
		this.id = id;
		this.name = name;
		this.designation = designation;
		this.salary = salary;
	}
	
}
